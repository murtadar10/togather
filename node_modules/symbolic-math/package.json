{
    "name": "symbolic-math",
    "version": "0.9.103",
    "description": "Computer Algebra System in TypeScript",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "system": "./dist/system/index.min.js",
            "default": "./dist/esm/index.min.js"
        }
    },
    "module": "./dist/esm/index.min.js",
    "types": "./dist/index.d.ts",
    "directories": {
        "test": "tests"
    },
    "scripts": {
        "build": "npm run clean && rollup --config rollup.config.mts --configPlugin @rollup/plugin-typescript",
        "clean": "rm -rf ./dist",
        "coverage": "nyc npm run test",
        "docs": "npm run docs:typedoc && open-cli docs/index.html",
        "docs:typedoc": "rm -rf docs && typedoc --options typedoc.json",
        "pages": "npm run docs:typedoc && gh-pages -d docs",
        "pretest": "npm run build",
        "prepublishOnly": "npm run units",
        "release": "release-it",
        "repl": "node repl.mjs",
        "test": "npm run units && npm run tests",
        "lint": "eslint . --ext .ts",
        "tests": "mocha 'tests/**/*.ts' --sort",
        "units": "mocha 'units/**/*.spec.ts' --sort",
        "unit-arg": "mocha 'units/arg.spec.ts'",
        "unit-clock": "mocha 'units/clock.spec.ts'",
        "unit-iscomplex": "mocha 'units/iscomplex.spec.ts'",
        "unit-sandbox": "mocha 'units/sandbox.spec.ts'",
        "test-sandbox": "mocha 'tests/sandbox.ts'"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/geometryzen/symbolic-math.git"
    },
    "keywords": [
        "algebrite",
        "algebra",
        "blade",
        "cas",
        "computer",
        "eigenmath",
        "geometric",
        "math",
        "multivector",
        "symbolic",
        "system"
    ],
    "author": {
        "name": "David Geo Holmes",
        "email": "david.geo.holmes@gmail.com"
    },
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/geometryzen/symbolic-math/issues"
    },
    "homepage": "https://github.com/geometryzen/symbolic-math#readme",
    "dependencies": {
        "big-integer": "^1.6.51",
        "generic-rbtree": "^1.1.2",
        "typhon-lang": "^1.0.13"
    },
    "devDependencies": {
        "@istanbuljs/nyc-config-typescript": "^1.0.2",
        "@rollup/plugin-commonjs": "^24.0.1",
        "@rollup/plugin-node-resolve": "^15.0.1",
        "@rollup/plugin-terser": "^0.4.0",
        "@rollup/plugin-typescript": "^11.0.0",
        "@types/chai": "^4.3.4",
        "@types/mocha": "^10.0.1",
        "@types/node": "^18.15.2",
        "@types/rollup-plugin-peer-deps-external": "^2.2.1",
        "@typescript-eslint/eslint-plugin": "^5.55.0",
        "@typescript-eslint/parser": "^5.55.0",
        "chai": "^4.3.7",
        "eslint": "^8.36.0",
        "gh-pages": "^5.0.0",
        "mocha": "^10.2.0",
        "nyc": "^15.1.0",
        "release-it": "^15.8.0",
        "rollup": "^3.19.1",
        "rollup-plugin-dts": "^5.2.0",
        "rollup-plugin-peer-deps-external": "^2.2.4",
        "ts-node": "^10.9.1",
        "typedoc": "^0.23.26",
        "typescript": "^4.9.5"
    }
}
